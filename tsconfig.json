{
	"compilerOptions": {
		"lib": ["esnext", "DOM"],
		"target": "esnext",
		"moduleResolution": "node",
		"module": "esnext",
		"resolveJsonModule": true,
		"allowJs": true,
		"checkJs": true,
		"strict": true,
		"noEmit": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true
	},
	"include": ["build.js"],
	"exclude": ["node_modules", "build"]
}